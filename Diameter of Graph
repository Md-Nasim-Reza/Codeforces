/// B. Diameter of Graph

#include<bits/stdc++.h>
#include<math.h>
#define ll long long
#define vl vector<ll>
#define vi vector<int>
#define v2 vector<vector<int>>
#define pb push_back
#define nl endl
#define yes "YES"
#define no "NO"
#define mod 1000000007
#define mod1 998244353
#define ull unsigned long long
#define     in_range(i,x,y)    for(int i=x;i<y;i++)
#define     in_range1(i,x,y)    for(int i=x;i<=y;i++)
#define st_input for(int i=x; i<y; i++) {cin>>st[i];}
using namespace std;
void solve(){
    ll n,i,j,a,b,c,k,x,y,z,m;
    cin>>n>>m>>k;
    if(m<(n-1))
        cout<<no<<nl;
    else if(m>(n*(n-1))/2)
        cout<<no<<nl;
    else if(k<=1)
        cout<<no<<nl;
    else if(k==2){
        if(n==1)
            cout<<yes<<nl;
        else cout<<no<<nl;
    }
    else if(k==3){
        if(m==(n*(n-1))/2)
            cout<<yes<<nl;
        else
            cout<<no<<nl;
    }
    else cout<<yes<<nl;
}
int main(){
    ll i,tc,n,ans;
    cin>>tc;
    while(tc--){
       solve();
    }
 
return 0;
}
