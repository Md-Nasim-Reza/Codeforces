/// Codeforces problem 1476A - K-divisible Sum


#include<bits/stdc++.h>
#include<math.h>
#include<string>
#define ll long long
#define vl vector<ll>
#define vi vector<int>
#define pb push_back
#define nl endl
#define yes "YES"
#define no "NO"
#define mod 1000000007
#define mod1 998244353
#define ull unsigned long long
using namespace std;

void solve(){
    ll n,m,u,v,i,j,k,x,y;
    cin>>n>>k;
   ll cf = (n+k-1)/k;
    k*=cf;
    cout<<(k+n-1)/n<<nl;

}

int main(){
    ll i,tc,n,Pow,m,ans;
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    cin>>tc;
    while(tc--){
        solve();
    }
return 0;
}
